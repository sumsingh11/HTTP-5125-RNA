@model CumulatievePart1.Models.Student

<h1>Student Details</h1>
<p><strong>Student ID:</strong> @Model.StudentId</p>
<p><strong>First Name:</strong> @Model.StudentFName</p>
<p><strong>Last Name:</strong> @Model.StudentLName</p>
<p><strong>Student Number:</strong> @Model.StudentNumber</p>
<p><strong>Enrollment Date:</strong> @Model.EnrolDate.ToString("yyyy-MM-dd")</p>